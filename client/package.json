{
  "name": "knooppuntnet-client",
  "version": "3.2.15",
  "license": "MIT",
  "scripts": {
    "start": "node --max_old_space_size=3092 ./node_modules/.bin/ng serve --proxy-config src/proxy-dev.conf.js",
    "ng": "ng",
    "build": "ng build",
    "test": "ng test",
    "lint": "node --max_old_space_size=8192 ./node_modules/.bin/ng lint",
    "extract-i18n": "ng extract-i18n --ivy --output-path src/locale && xliffmerge --profile xliffmerge.json",
    "start:nl": "node --max_old_space_size=3092 ./node_modules/.bin/ng serve --configuration=nl --proxy-config src/proxy.conf.js",
    "start:de": "node --max_old_space_size=3092 ./node_modules/.bin/ng serve --configuration=de --proxy-config src/proxy.conf.js",
    "start:fr": "node --max_old_space_size=3092 ./node_modules/.bin/ng serve --configuration=fr --proxy-config src/proxy.conf.js",
    "test-no-watch": "ng test --single-run",
    "buildstats": "rm -rf ./dist/*; node --max_old_space_size=3092 ./node_modules/.bin/ng build --prod --build-optimizer --stats-json",
    "analyze": "webpack-bundle-analyzer dist/knooppuntnet/en/stats.json",
    "build-prod": "rm -rf ./dist/*; node --max_old_space_size=3092 ./node_modules/.bin/ng build --prod --build-optimizer --progress --output-path ./dist/${npm_package_name}-${npm_package_version};cd dist/${npm_package_name}-${npm_package_version}; gzip --keep */*.js */*.json */*.css",
    "zip": "cd dist; tar -czf ../../dist/${npm_package_name}-${npm_package_version}.tar.gz .",
    "dist": "run-s build-prod zip"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.13",
    "@angular/cdk": "^11.2.12",
    "@angular/common": "^11.2.13",
    "@angular/compiler": "^11.2.13",
    "@angular/core": "^11.2.13",
    "@angular/forms": "^11.2.13",
    "@angular/localize": "~11.2.13",
    "@angular/material": "^11.2.12",
    "@angular/platform-browser": "^11.2.13",
    "@angular/platform-browser-dynamic": "^11.2.13",
    "@angular/router": "^11.2.13",
    "@angular/service-worker": "^11.2.13",
    "@ngrx/effects": "^11.1.1",
    "@ngrx/router-store": "^11.1.1",
    "@ngrx/store": "^11.1.1",
    "@ngrx/store-devtools": "^11.1.1",
    "@sentry/angular": "^6.12.0",
    "@swimlane/ngx-charts": "^16.0.0",
    "@types/file-saver": "^2.0.0",
    "angular-qrious": "9.0.0",
    "canvg": "^1.5.3",
    "core-js": "^2.5.4",
    "file-saver-es": "^2.0.5",
    "immutable": "^4.0.0-rc.12",
    "jspdf": "^2.3.1",
    "maplibre-gl": "1.15.2",
    "ngrx-store-localstorage": "^11.0.0",
    "ngx-clipboard": "^14.0.1",
    "ngx-i18nsupport": "^0.17.1",
    "ngx-markdown": "11.0.1",
    "ol": "^6.5.1-dev.1617840589399",
    "ol-geocoder": "4.1.2",
    "qrious": "^4.0.2",
    "rxjs": "^6.6.7",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.12",
    "@angular-eslint/builder": "^4.0.0",
    "@angular-eslint/eslint-plugin": "^4.0.0",
    "@angular-eslint/eslint-plugin-template": "^4.0.0",
    "@angular-eslint/schematics": "4.2.0",
    "@angular-eslint/template-parser": "^4.0.0",
    "@angular/cli": "^11.2.12",
    "@angular/compiler-cli": "^11.2.13",
    "@angular/language-service": "^11.2.13",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.6",
    "@types/mapbox-gl": "^1.10.2",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "cypress": "^3.8.3",
    "eslint": "^7.23.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "svg-inline-loader": "^0.8.0",
    "ts-node": "~8.0.3",
    "tslint": "~6.1.0",
    "typescript": "4.0.5",
    "webpack-bundle-analyzer": "^4.3.0"
  },
  "cli": {
    "packageManager": "yarn"
  }
}
