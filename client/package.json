{
  "name": "knooppuntnet-client",
  "version": "4.1.0",
  "license": "MIT",
  "scripts": {
    "prettier-check": "prettier --check --config .prettierrc --ignore-path .prettierignore --loglevel warn \"{**/*,*}.{js,ts}\"",
    "prettier-write": "prettier --write --config .prettierrc --ignore-path .prettierignore --loglevel warn \"{**/*,*}.{js,ts}\"",
    "extract-i18n": "nx extract-i18n --output-path src/locale",
    "start:nl": "nx run serve --configuration=nl --proxy-config src/proxy-dev.conf.js",
    "start:de": "nx run serve --configuration=de --proxy-config src/proxy-dev.conf.js",
    "start:fr": "nx run serve --configuration=fr --proxy-config src/proxy-dev.conf.js",
    "test-no-watch": "nx test --single-run",
    "buildstats": "rm -rf ./dist/*; nx run knooppuntnet:build:production --build-optimizer --stats-json",
    "analyze": "webpack-bundle-analyzer dist/knooppuntnet/en/stats.json",
    "build-prod": "rm -rf ./dist/*; nx run knooppuntnet:build:production --build-optimizer --progress --output-path ./dist/${npm_package_name}-${npm_package_version};cd dist/${npm_package_name}-${npm_package_version}; gzip --keep */*.js */*.json */*.css",
    "zip": "cd dist; tar -czf ../../dist/${npm_package_name}-${npm_package_version}.tar.gz .",
    "dist": "run-s build-prod zip"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.6",
    "@angular/cdk": "^15.2.6",
    "@angular/common": "^15.2.6",
    "@angular/compiler": "^15.2.6",
    "@angular/core": "^15.2.6",
    "@angular/forms": "^15.2.6",
    "@angular/localize": "^15.2.6",
    "@angular/material": "^15.2.6",
    "@angular/platform-browser": "^15.2.6",
    "@angular/platform-browser-dynamic": "^15.2.6",
    "@angular/router": "^15.2.6",
    "@angular/service-worker": "^15.2.6",
    "@ngrx/effects": "15.4.0",
    "@ngrx/eslint-plugin": "15.4.0",
    "@ngrx/router-store": "15.4.0",
    "@ngrx/store": "15.4.0",
    "@ngrx/store-devtools": "15.4.0",
    "@sentry/angular-ivy": "^7.46.0",
    "@swimlane/ngx-charts": "^20.1.0",
    "@types/file-saver": "^2.0.5",
    "canvg": "^4.0.1",
    "core-js": "^3.25.0",
    "file-saver-es": "^2.0.5",
    "immutable": "^4.1.0",
    "jspdf": "^2.5.1",
    "marked": "4.0.17",
    "ngrx-store-localstorage": "^15.0.0",
    "ngx-clipboard": "^15.1.0",
