{
  "name": "knooppuntnet-client",
  "version": "4.0.28",
  "license": "MIT",
  "scripts": {
    "start": "node --max_old_space_size=3092 ./node_modules/.bin/ng serve --host 127.0.0.1 --port 4000 --proxy-config src/proxy-dev.conf.js",
    "lint": "node --max_old_space_size=8192 ./node_modules/.bin/ng lint",
    "ng": "ng",
    "build": "ng build",
    "test": "ng test",
    "prettier-check": "prettier --check --config .prettierrc --ignore-path .prettierignore --loglevel warn \"{**/*,*}.{js,ts}\"",
    "prettier-write": "prettier --write --config .prettierrc --ignore-path .prettierignore --loglevel warn \"{**/*,*}.{js,ts}\"",
    "extract-i18n": "ng extract-i18n --output-path src/locale",
    "start:nl": "node --max_old_space_size=3092 ./node_modules/.bin/ng serve --configuration=nl --proxy-config src/proxy-dev.conf.js",
    "start:de": "node --max_old_space_size=3092 ./node_modules/.bin/ng serve --configuration=de --proxy-config src/proxy-dev.conf.js",
    "start:fr": "node --max_old_space_size=3092 ./node_modules/.bin/ng serve --configuration=fr --proxy-config src/proxy-dev.conf.js",
    "test-no-watch": "ng test --single-run",
    "buildstats": "rm -rf ./dist/*; node --max_old_space_size=3092 ./node_modules/.bin/ng build --configuration production --build-optimizer --stats-json",
    "analyze": "webpack-bundle-analyzer dist/knooppuntnet/en/stats.json",
    "build-prod": "rm -rf ./dist/*; node --max_old_space_size=3092 ./node_modules/.bin/ng build --configuration production --build-optimizer --progress --output-path ./dist/${npm_package_name}-${npm_package_version};cd dist/${npm_package_name}-${npm_package_version}; gzip --keep */*.js */*.json */*.css",
    "zip": "cd dist; tar -czf ../../dist/${npm_package_name}-${npm_package_version}.tar.gz .",
    "dist": "run-s build-prod zip"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/localize": "^15.0.0",
    "@angular/material": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@angular/service-worker": "^15.0.0",
    "@ngrx/effects": "^14.3.1",
    "@ngrx/router-store": "^14.3.1",
    "@ngrx/store": "^14.3.1",
    "@ngrx/store-devtools": "^14.3.1",
    "@sentry/angular": "^7.13.0",
    "@swimlane/ngx-charts": "^20.1.0",
    "@types/file-saver": "^2.0.5",
    "canvg": "^4.0.1",
    "core-js": "^3.25.0",
    "file-saver-es": "^2.0.5",
    "immutable": "^4.1.0",
    "jspdf": "^2.5.1",
    "maplibre-gl": "1.15.2",
    "ngrx-store-localstorage": "^14.0.0",
    "ngx-clipboard": "^15.1.0",
    "ngx-cookie-service": "^14.0.1",
    "ngx-device-detector": "^4.0.1",
    "ngx-markdown": "14.0.1",
    "ol": "6.6.1",
    "ol-geocoder": "4.1.2",
    "rxjs": "^7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular-eslint/builder": "^15.1.0",
    "@angular-eslint/eslint-plugin": "^15.1.0",
    "@angular-eslint/eslint-plugin-template": "^15.1.0",
    "@angular-eslint/schematics": "15.1.0",
    "@angular-eslint/template-parser": "^15.1.0",
    "@angular/cli": "^15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@angular/language-service": "^15.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/jasminewd2": "^2.0.6",
    "@types/mapbox-gl": "^1.10.2",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "eslint": "8.22.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.6",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "svg-inline-loader": "^0.8.2",
    "ts-node": "~10.9.1",
    "typescript": "4.8.4",
    "webpack-bundle-analyzer": "^4.6.1"
  },
  "cli": {
    "packageManager": "yarn"
  }
}