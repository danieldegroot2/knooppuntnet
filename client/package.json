{
  "name": "knooppuntnet-client",
  "version": "3.0.0-alpha-8",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "extract-i18n": "ng xi18n --output-path locale && xliffmerge --profile xliffmerge.json nl de fr",
    "start": "ng serve --baseHref=en --open --proxy-config src/proxy.conf.js --host 0.0.0.0 --disable-host-check",
    "start:nl": "ng serve --baseHref=nl --configuration=nl --open --proxy-config src/proxy.conf.js --host 0.0.0.0 --disable-host-check",
    "start:de": "ng serve --baseHref=de --configuration=de --open --proxy-config src/proxy.conf.js --host 0.0.0.0 --disable-host-check",
    "start:fr": "ng serve --baseHref=fr --configuration=fr --open --proxy-config src/proxy.conf.js --host 0.0.0.0 --disable-host-check",
    "test-no-watch": "ng test --single-run",
    "build-dev": "ng build --progress --output-path ./dist",
    "build-prod": "ng build --base-href /en/ --extract-css --progress --prod --aot --output-path ./dist/${npm_package_name}-${npm_package_version}/en;cd dist/${npm_package_name}-${npm_package_version}/en; gzip --keep *.js *.json *.css",
    "build-prod:nl": "ng build --base-href /nl/ --configuration=production-nl --extract-css --progress --prod --aot --output-path ./dist/${npm_package_name}-${npm_package_version}/nl;cd dist/${npm_package_name}-${npm_package_version}/nl; gzip --keep *.js *.json *.css",
    "build-prod:de": "ng build --base-href /de/ --configuration=production-de --extract-css --progress --prod --aot --output-path ./dist/${npm_package_name}-${npm_package_version}/de;cd dist/${npm_package_name}-${npm_package_version}/de; gzip --keep *.js *.json *.css",
    "build-prod:fr": "ng build --base-href /fr/ --configuration=production-fr --extract-css --progress --prod --aot --output-path ./dist/${npm_package_name}-${npm_package_version}/fr;cd dist/${npm_package_name}-${npm_package_version}/fr; gzip --keep *.js *.json *.css",
    "build-map": "ng build --base-href /en/ --extract-css --progress --prod --aot --output-path ./map --stats-json --source-map",
    "zip": "cd dist; tar -czf ../../dist/${npm_package_name}-${npm_package_version}.tar.gz .",
    "dist": "run-s build-prod build-prod:nl build-prod:de build-prod:fr zip"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "^9.0.0",
    "@angular/compiler": "^9.0.0",
    "@angular/core": "^9.0.0",
    "@angular/forms": "^9.0.0",
    "@angular/localize": "~9.0.0",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "^9.0.0",
    "@angular/platform-browser-dynamic": "^9.0.0",
    "@angular/router": "^9.0.0",
    "@angular/service-worker": "^9.0.0",
    "@types/file-saver": "^2.0.0",
    "@types/jspdf": "^1.3.3",
    "@types/ol": "^6.1.0",
    "canvg": "^1.5.3",
    "core-js": "^2.5.4",
    "file-saver": "^2.0.1",
    "immutable": "^4.0.0-rc.12",
    "jspdf": "^1.5.3",
    "ngx-i18nsupport": "^0.17.1",
    "ngx-markdown": "8.2.2",
    "ol": "^6.1.1",
    "ol-geocoder": "4.0.0",
    "ol-mapbox-style": "6.0.0",
    "qrious": "^4.0.2",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "^9.0.1",
    "@angular/compiler-cli": "^9.0.0",
    "@angular/language-service": "^9.0.0",
    "@types/jasmine": "^3.3.12",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "cypress": "^3.8.3",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.2",
    "svg-inline-loader": "^0.8.0",
    "ts-node": "~8.0.3",
    "tslint": "c5.15.0",
    "typescript": "3.7.5"
  },
  "cli": {
    "packageManager": "yarn"
  }
}
