{
  "name": "knooppuntnet-client",
  "version": "4.0.0-snapshot.6",
  "license": "MIT",
  "scripts": {
    "start": "node --max_old_space_size=3092 ./node_modules/.bin/ng serve --proxy-config src/proxy-dev.conf.js",
    "ng": "ng",
    "build": "ng build",
    "test": "ng test",
    "lint": "node --max_old_space_size=8192 ./node_modules/.bin/ng lint",
    "extract-i18n": "ng extract-i18n --output-path src/locale && xliffmerge --profile xliffmerge.json",
    "start:nl": "node --max_old_space_size=3092 ./node_modules/.bin/ng serve --configuration=nl --proxy-config src/proxy-dev.conf.js",
    "start:de": "node --max_old_space_size=3092 ./node_modules/.bin/ng serve --configuration=de --proxy-config src/proxy-dev.conf.js",
    "start:fr": "node --max_old_space_size=3092 ./node_modules/.bin/ng serve --configuration=fr --proxy-config src/proxy-dev.conf.js",
    "test-no-watch": "ng test --single-run",
    "buildstats": "rm -rf ./dist/*; node --max_old_space_size=3092 ./node_modules/.bin/ng build --configuration production --build-optimizer --stats-json",
    "analyze": "webpack-bundle-analyzer dist/knooppuntnet/en/stats.json",
    "build-prod": "rm -rf ./dist/*; node --max_old_space_size=3092 ./node_modules/.bin/ng build --configuration production --build-optimizer --progress --output-path ./dist/${npm_package_name}-${npm_package_version};cd dist/${npm_package_name}-${npm_package_version}; gzip --keep */*.js */*.json */*.css",
    "zip": "cd dist; tar -czf ../../dist/${npm_package_name}-${npm_package_version}.tar.gz .",
    "dist": "run-s build-prod zip"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.2",
    "@angular/cdk": "^14.0.2",
    "@angular/common": "^14.0.2",
    "@angular/compiler": "^14.0.2",
    "@angular/core": "^14.0.2",
    "@angular/forms": "^14.0.2",
    "@angular/localize": "^14.0.2",
    "@angular/material": "^14.0.2",
    "@angular/platform-browser": "^14.0.2",
    "@angular/platform-browser-dynamic": "^14.0.2",
    "@angular/router": "^14.0.2",
    "@angular/service-worker": "^14.0.2",
    "@ngrx/effects": "^13.2.0",
    "@ngrx/router-store": "^13.2.0",
    "@ngrx/store": "^13.2.0",
    "@ngrx/store-devtools": "^13.2.0",
    "@sentry/angular": "^7.1.1",
    "@swimlane/ngx-charts": "^20.1.0",
    "@types/file-saver": "^2.0.5",
    "angular-qrious": "9.0.0",
    "canvg": "^4.0.0",
    "core-js": "^3.23.1",
    "file-saver-es": "^2.0.5",
    "immutable": "^4.1.0",
    "jspdf": "^2.5.1",
    "maplibre-gl": "1.15.2",
    "ngrx-store-localstorage": "^13.0.1",
    "ngx-clipboard": "^15.1.0",
    "ngx-markdown": "14.0.0",
    "ol": "^6.6.1",
    "ol-geocoder": "4.1.2",
    "qrious": "^4.0.2",
    "rxjs": "^7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.2",
    "@angular-eslint/builder": "^13.5.0",
    "@angular-eslint/eslint-plugin": "^13.5.0",
    "@angular-eslint/eslint-plugin-template": "^13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "^13.5.0",
    "@angular/cli": "^14.0.2",
    "@angular/compiler-cli": "^14.0.2",
    "@angular/language-service": "^14.0.2",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "^2.0.6",
    "@types/mapbox-gl": "^1.10.2",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "eslint": "^8.17.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "~4.2.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "svg-inline-loader": "^0.8.2",
    "ts-node": "~10.8.1",
    "tslint": "~6.1.0",
    "typescript": "4.7.3",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "cli": {
    "packageManager": "yarn"
  }
}
