{
  "name": "knooppuntnet-client",
  "version": "3.0.0-alpha-18",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "extract-i18n": "ng xi18n --output-path locale && xliffmerge --profile xliffmerge.json",
    "start": "node --max_old_space_size=8192 ./node_modules/.bin/ng serve --open --proxy-config src/proxy-dev.conf.js --host 0.0.0.0 --disable-host-check",
    "start:nl": "node --max_old_space_size=8192 ./node_modules/.bin/ng serve --configuration=nl --open --proxy-config src/proxy.conf.js --host 0.0.0.0 --disable-host-check",
    "start:de": "node --max_old_space_size=8192 ./node_modules/.bin/ng serve --configuration=de --open --proxy-config src/proxy.conf.js --host 0.0.0.0 --disable-host-check",
    "start:fr": "node --max_old_space_size=8192 ./node_modules/.bin/ng serve --configuration=fr --open --proxy-config src/proxy.conf.js --host 0.0.0.0 --disable-host-check",
    "test-no-watch": "ng test --single-run",
    "build-prod": "node --max_old_space_size=8192 ./node_modules/.bin/ng build --prod --build-optimizer --progress --output-path ./dist/${npm_package_name}-${npm_package_version};cd dist/${npm_package_name}-${npm_package_version}; gzip --keep */*.js */*.json */*.css",
    "zip": "cd dist; tar -czf ../../dist/${npm_package_name}-${npm_package_version}.tar.gz .",
    "dist": "run-s build-prod zip"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/cdk": "^9.1.2",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/localize": "~9.1.0",
    "@angular/material": "^9.1.2",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/router": "^9.1.0",
    "@angular/service-worker": "^9.1.0",
    "@swimlane/ngx-charts": "^13.0.2",
    "@types/file-saver": "^2.0.0",
    "@types/ol": "^6.1.0",
    "canvg": "^1.5.3",
    "core-js": "^2.5.4",
    "file-saver": "^2.0.1",
    "immutable": "^4.0.0-rc.12",
    "jspdf": "^1.5.3",
    "ngx-i18nsupport": "^0.17.1",
    "ngx-markdown": "9.0.0",
    "ol": "^6.3.1",
    "ol-geocoder": "4.0.0",
    "ol-mapbox-style": "6.0.1",
    "qrious": "^4.0.2",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "^9.1.0",
    "@angular/compiler-cli": "^9.1.0",
    "@angular/language-service": "^9.1.0",
    "@types/jasmine": "^3.3.12",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "cypress": "^3.8.3",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.2",
    "svg-inline-loader": "^0.8.0",
    "ts-node": "~8.0.3",
    "tslint": "c5.15.0",
    "typescript": "3.7.5"
  },
  "cli": {
    "packageManager": "yarn"
  }
}
