[
  {
    "$unwind": {
      "path": "$subsets"
    }
  },
  {
    "$group": {
      "_id": {
        "country": "$subsets.country",
        "networkType": "$subsets.networkType"
      },
      "changeCount": {
        "$sum": 1
      }
    }
  },
  {
    "$project": {
      "_id": {"$concat": ["$_id.country", ":", "$_id.networkType", ":ChangeCount"]},
      "country": "$_id.country",
      "networkType": "$_id.networkType",
      "name": "ChangeCount",
      "value": "$changeCount"
    }
  },
  {
    "$merge": {
      "into": "statistics-subset-change-count",
      "on": "_id"
    }
  }
]