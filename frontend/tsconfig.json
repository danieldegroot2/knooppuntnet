{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    // "strict": true, TODO re-enable?
    "noImplicitOverride": true,
    // TODO re-enable noPropertyAccessFromIndexSignature?
    "noPropertyAccessFromIndexSignature": false,
    // TODO re-enable noImplicitReturns?
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    // TODO re-enable strictPropertyInitialization?
    "strictPropertyInitialization": false,
    // TODO re-enable strictNullChecks?
    "strictNullChecks": false,
    // TODO re-enable libCheck?
    "skipLibCheck": true,
    "lib": [
      "ES2022",
      "dom"
    ],
    "paths": {
      "@api/*": [
        "src/app/api/*"
      ],
      "@app/*": [
        "src/app/index.ts"
      ],
      "@app/analysis/analysis": [
        "src/app/analysis/analysis/index.ts"
      ],
      "@app/analysis/changeset": [
        "src/app/analysis/changeset/index.ts"
      ],
      "@app/analysis/components/change-set": [
        "src/app/analysis/components/change-set/index.ts"
      ],
      "@app/analysis/components/change-set/components": [
        "src/app/analysis/components/change-set/components/index.ts"
      ],
      "@app/analysis/components/changes": [
        "src/app/analysis/components/changes/index.ts"
      ],
      "@app/analysis/components/changes/filter": [
        "src/app/analysis/components/changes/filter/index.ts"
      ],
      "@app/analysis/components/changes/node": [
        "src/app/analysis/components/changes/node/index.ts"
      ],
      "@app/analysis/components/changes/route": [
        "src/app/analysis/components/changes/route/index.ts"
      ],
      "@app/analysis/components/edit": [
        "src/app/analysis/components/edit/index.ts"
      ],
      "@app/analysis/components/filter": [
        "src/app/analysis/components/filter/index.ts"
      ],
      "@app/analysis/components/indicators/route": [
        "src/app/analysis/components/indicators/route/index.ts"
      ],
      "@app/analysis/fact": [
        "src/app/analysis/fact/index.ts"
      ],
      "@app/analysis/facts": [
        "src/app/analysis/facts/index.ts"
      ],
      "@app/analysis/location": [
        "src/app/analysis/location/index.ts"
      ],
      "@app/analysis/network": [
        "src/app/analysis/network/index.ts"
      ],
      "@app/analysis/node": [
        "src/app/analysis/node/index.ts"
      ],
      "@app/analysis/overview": [
        "src/app/analysis/overview/index.ts"
      ],
      "@app/analysis/route": [
        "src/app/analysis/route/index.ts"
      ],
      "@app/analysis/strategy": [
        "src/app/analysis/strategy/index.ts"
      ],
      "@app/analysis/subset": [
        "src/app/analysis/subset/index.ts"
      ],
      "@app/components/poi": [
        "src/app/shared/components/poi/index.ts"
      ],
      "@app/components/shared": [
        "src/app/shared/components/shared/index.ts"
      ],
      "@app/components/shared/data": [
        "src/app/shared/components/shared/data/index.ts"
      ],
