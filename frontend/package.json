{
  "name": "frontend",
  "version": "4.2.1",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve --proxy-config src/proxy-dev.conf.js",
    "serve:nl": "ng serve --configuration=nl --proxy-config src/proxy-dev.conf.js",
    "serve:de": "ng serve --configuration=de --proxy-config src/proxy-dev.conf.js",
    "serve:fr": "ng serve --configuration=fr --proxy-config src/proxy-dev.conf.js",
    "extract-i18n": "ng extract-i18n --output-path src/locale",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-prod": "rm -rf ./dist/*; ng run frontend:build:production --build-optimizer --progress --source-map --output-path ./dist/${npm_package_name}-${npm_package_version};cd dist/${npm_package_name}-${npm_package_version}; gzip --keep */*.js */*.json */*.css",
    "zip": "cd dist; tar -czf ../../dist/${npm_package_name}-${npm_package_version}.tar.gz .",
    "dist": "run-s build-prod zip"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.2",
    "@angular/cdk": "17.0.0",
    "@angular/common": "^17.0.2",
    "@angular/compiler": "^17.0.2",
    "@angular/core": "^17.0.2",
    "@angular/forms": "^17.0.2",
    "@angular/localize": "17.0.2",
    "@angular/material": "17.0.0",
    "@angular/platform-browser": "^17.0.2",
    "@angular/platform-browser-dynamic": "^17.0.2",
    "@angular/router": "^17.0.2",
    "@angular/service-worker": "17.0.2",
    "@ngrx/effects": "16.2.0",
    "@ngrx/eslint-plugin": "16.2.0",
    "@ngrx/router-store": "16.2.0",
    "@ngrx/store": "16.2.0",
    "@ngrx/store-devtools": "16.2.0",
    "@sentry/angular-ivy": "^7.69.0",
    "@swimlane/ngx-charts": "^20.5.0",
    "@types/file-saver": "^2.0.5",
    "canvg": "^4.0.1",
    "file-saver-es": "^2.0.5",
    "immutable": "^5.0.0-beta.4",
    "jspdf": "^2.5.1",
    "marked": "^4.3.0",
    "ngrx-store-localstorage": "^16.0.0",
    "ngx-clipboard": "^16.0.0",
    "ngx-cookie-service": "^16.0.1",
    "ngx-device-detector": "^6.0.2",
    "ngx-markdown": "16.0.0",
    "ol": "8.1.0",
    "ol-geocoder": "4.3.1",
    "ol-mapbox-style": "12.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular-devkit/core": "17.0.0",
    "@angular-devkit/schematics": "17.0.0",
    "@angular-eslint/builder": "^16.1.2",
    "@angular-eslint/eslint-plugin": "16.1.2",
    "@angular-eslint/eslint-plugin-template": "16.1.2",
    "@angular-eslint/schematics": "16.1.2",
    "@angular-eslint/template-parser": "16.1.2",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.2",
    "@angular/language-service": "17.0.2",
    "@schematics/angular": "16.2.2",
    "@softarc/eslint-plugin-sheriff": "^0.13.1",
    "@softarc/sheriff-core": "^0.13.1",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "svg-inline-loader": "^0.8.2",
    "ts-node": "~10.9.1",
    "typescript": "5.2.2"
  }
}
